<section id="panel">
<section id="panelCalendarBox">

	<h1>{{ MonthName }} {{ CurrentYear }}</h1>

	<div id="panelCalendarContent">

		<div id="panelCalendarNav" class="control">
			<table>
				<tr>
					<td class="today">
						{% trans %}today{% endtrans %}
					</td>
					<td class="secondary">
						<a href="{{ MonthPrevYearUrl }}"  title="{% trans %}Previous year{% endtrans %}" class="secondary">&lt;&lt;</a>
					</td>
					<td>
						<a href="{{ MonthPrevMonthUrl }}" title="{% trans %}Previous month{% endtrans %}">&lt;</a>
					</td>
					<td>
						<a href="{{ MonthNextMonthUrl }}" title="{% trans %}Next month{% endtrans %}">&gt;</a>
					</td>
					<td class="secondary">
						<a href="{{ MonthNextYearUrl }}"  title="{% trans %}Next year{% endtrans %}" class="secondary">&gt;&gt;</a>
					</td>
				</tr>
			</table>
		</div>

		<div id="panelCalendarMode" class="control">
			<table width="100%">
				<tr>
					<td title="{% trans %}Month{% endtrans %}">
						{% trans %}Month{% endtrans %}
					</td>
					<td>
						<a href="{{ ModeWeekUrl }}" title="{% trans %}Week{% endtrans %}">{% trans %}Week{% endtrans %}</a>
					</td>
					<td>
						<a href="{{ ModeDayUrl }}" title="{% trans %}Day{% endtrans %}">{% trans %}Day{% endtrans %}</a>
					</td>
				</tr>
			</table>
		</div>


		<h2>quick navigation</h2>

		<div id="panelCalendar">
			{% include 'TimeTMCalendarBundle:Default:panelCalendar.html.twig' %}
		</div>
	</div>
</section>
</section>