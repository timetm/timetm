<section id="ttm_panel">
<section id="panelCalendarBox">

	<div id="dateDisplay">
        {{ 'nav.week'|trans }} {{ WeekStamp }}
        <span class="hideForBig" title="close"><i class="icon-cancel"></i></span>
    </div>

	<div id="panelCalendarContent">

		<div id="panelCalendarNav" class="controls">
			<table width="100%">
				<tr>
					<td class="today">
						<a href="{{ path('week_no_param') }}">{{ 'nav.today'|trans }}</a>
					</td>
					<td class="secondary">
						<a href="{{ WeekPrevYearUrl }}" title="{{ 'nav.previous.year'|trans }}">&lt;&lt;</a>
					</td>
					<td>
						<a href="{{ WeekPrevWeekUrl }}" title="{{ 'nav.previous.week'|trans }}">&lt;</a>
					</td>
					<td>
						<a href="{{ WeekNextWeekUrl }}" title="{{ 'nav.next.week'|trans }}">&gt;</a>
					</td>
					<td class="secondary">
						<a href="{{ WeekNextYearUrl }}" title="{{ 'nav.next.year'|trans }}">&gt;&gt;</a>
					</td>
				</tr>
			</table>
		</div>

		<div id="panelCalendarMode" class="controls">
			<table width="100%">
				<tr>
					<td>
						<a href="{{ ModeMonthUrl }}" title="{{ 'nav.month'|trans }}">{{ 'nav.month'|trans }}</a>
					</td>
					<td title="{{ 'nav.week'|trans }}">{{ 'nav.week'|trans }}</td>
					<td>
						<a href="{{ ModeDayUrl }}"  title="{{ 'nav.day'|trans }}">{{ 'nav.day'|trans }}</a>
					</td>
				</tr>
			</table>
		</div>

		<h2>quick navigation</h2>

		<div id="panelCalendar">
			{% include 'TimeTMCoreBundle:Calendar:Default/panelCalendar.html.twig' %}
		</div>
	</div>
</section>
</section>
