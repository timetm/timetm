{# src/Lookin2/CalendarBundle/Resources/views/Default/month.html.twig #}
{% extends '::base.html.twig' %}

{% block title %}
calendar {{ CurrentMonth }}
{% endblock %}


{% block mainContent %}
	{% include 'Lookin2CalendarBundle:Default:calendarContent.html.twig' 
		with {
			'days'         : days,
			'PrevYearUrl'  : PrevYearUrl,
			'PrevMonthUrl' : PrevMonthUrl,
			'CurrentMonth' : CurrentMonth,
			'NextMonthUrl' : NextMonthUrl,
			'NextYearUrl'  : NextYearUrl,
		} 
	%}
{% endblock %}